<mvc:View
  controllerName="ui2.choosingtech.controller.StartPage"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:sf="sap.f"
  height="100%"
  width="100%"
>
  <sf:DynamicPage
    showFooter="true"
    stickySubheaderProvider="idWizard"
    class="sapUiNoContentPadding"
    fitContent="true"
  >
    <sf:title>
      <sf:DynamicPageTitle primaryArea="Middle">
        <sf:heading>
          <Title text="Выбор целевой технологии" />
        </sf:heading>
      </sf:DynamicPageTitle>
    </sf:title>
    <sf:header>
    </sf:header>
    <sf:content>
      <Wizard id="idWizard" enableBranching="true" showNextButton="false"
              class="sapUiResponsivePadding--header sapUiResponsivePadding--content"
              steps="{ path: '/nodes', factory: '.factorySteps' }">
      </Wizard>
    </sf:content>
    <sf:footer>

    </sf:footer>
  </sf:DynamicPage>
</mvc:View>